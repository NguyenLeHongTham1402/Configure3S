@model IEnumerable<Configure3S.Models.TbApiconfigure>

@{
    ViewData["Title"] = "ListAPIConfig";
}

<h1>ListAPIConfig</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
@if (Model != null)
{
    <table class="table">
        <thead>
            <tr>
                <th>
                    @Html.DisplayNameFor(model => model.CompanyId)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.Apikey)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.Urlapi)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.UserName)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.Password)
                </th>
                <th></th>
            </tr>
        </thead>
        <tbody>
            @foreach (var item in Model)
            {
                <tr>
                    <td>
                        @Html.DisplayFor(modelItem => item.CompanyId)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.Apikey)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.Urlapi)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.UserName)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.Password)
                    </td>
                    <td>
                        <a asp-action="Edit" asp-route-id="@item.SourceId">Edit</a> |
                        <a asp-action="Details" asp-route-id="@item.SourceId">Details</a> |
                        <a asp-action="Delete" asp-route-id="@item.SourceId">Delete</a>
                    </td>
                </tr>
            }
        </tbody>
    </table>
}

